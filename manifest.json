{
  "manifest_version": 3,
  "name": "cmdb",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script/main.tsx"],
      "media": []
    }
  ],
  "background": {
    "service_worker": "background/index.ts",
    "persistent": false
  },
  "permissions": ["activeTab", "tabs", "bookmarks", "notifications", "storage"],
  "commands": {
    "run-foo": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "Run \"Cmd+B\" on the current page."
    },
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Command+B",
        "chromeos": "Ctrl+B",
        "linux": "Ctrl+B"
      }
    }
  }
}
