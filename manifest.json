{
  "manifest_version": 3,
  "name": "bkmrk",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script/main.tsx"],
      "media": []
    }
  ],
  "background": {
    "service_worker": "background/index.ts"
  },
  "permissions": ["activeTab", "bookmarks"]
}
